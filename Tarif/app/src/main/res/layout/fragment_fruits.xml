        android:textColor="@color/white"
        android:text="Kiraz\n"
        app:layout_constraintBottom_toTopOf="@+id/uzum"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/erik">

    </CheckBox>

    <CheckBox
        android:id="@+id/limon"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="Limon\n"
        android:textSize="20sp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/uzum"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/kiraz">

    </CheckBox>

    <CheckBox
        android:id="@+id/incir"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="İncir\n"
        android:textSize="20sp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/erik"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/avokado">

    </CheckBox>

    <CheckBox
        android:id="@+id/uzum"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="Üzüm\n"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/hurma"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kiraz">

    </CheckBox>

    <CheckBox
        android:id="@+id/hurma"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="Hurma\n"
        android:textSize="20sp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/mango"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/uzum">

    </CheckBox>

    <CheckBox
        android:id="@+id/dut"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:textSize="20sp"
        android:textColor="@color/white"
        android:text=" Dut\n"
        app:layout_constraintBottom_toTopOf="@+id/alic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
