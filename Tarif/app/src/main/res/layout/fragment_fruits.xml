        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/greyfurt">

    </CheckBox>

    <CheckBox
        android:id="@+id/kivi"
        android:layout_width="match_parent"
        android:layout_height="35dp"
