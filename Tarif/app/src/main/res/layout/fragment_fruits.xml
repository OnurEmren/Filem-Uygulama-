        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kivi">

    </CheckBox>

    <CheckBox
        android:id="@+id/cilek"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="Çilek\n"
        android:textSize="20sp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/avokado"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ananas">

    </CheckBox>

    <CheckBox
        android:id="@+id/avokado"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="Avokado\n"
        android:textSize="20sp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/incir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cilek">

    </CheckBox>

    <CheckBox
        android:id="@+id/erik"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="Erik\n"
        android:textSize="20sp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/kiraz"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/incir">

    </CheckBox>

    <CheckBox
        android:id="@+id/kiraz"
        android:layout_width="match_parent"
        android:layout_height="35dp"
