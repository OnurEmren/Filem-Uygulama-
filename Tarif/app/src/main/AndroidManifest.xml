<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.onuremren.tarif">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/shopping"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/shopping"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tarif">
        <activity
            android:name=".view.activity.MainActivity3"
            android:exported="false" />
        <activity
            android:name=".view.activity.MainActivity2"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.ShoppingListActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".view.activity.MainActivity"
            android:exported="true"></activity>
    </application>

</manifest>